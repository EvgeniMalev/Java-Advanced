// Count integers of a queue

import java.util.LinkedList;
import java.util.Queue;

public class Static integers {
    public static void main(String[] args) {
        Queue<Integer> queue = new LinkedList<>();

        // Add elements to the queue
        queue.add(5);
        queue.add(2);
        queue.add(3);
        queue.add(8);
        queue.add(24);
        queue.add(108);
        // Count the elements in the queue
        int count = countElements(queue);

        // Display the result
        System.out.println("Number of elements in the queue: " + count);
    }

    public static <T> int countElements(Queue<T> queue) {
        int count = queue.size();
        return count;
    }
}
